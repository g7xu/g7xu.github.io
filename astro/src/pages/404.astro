---
import BaseLayout from '@layouts/BaseLayout.astro';

export const prerender = true;
---

<BaseLayout title="Page Not Found">
  <section>
    <h1>Page Not Found</h1>
    <p>Page not found. Your pixels are in another canvas.</p>
    <p>
      Sorry, but the page you were trying to view does not exist. You can head
      back to the <a href="/">homepage</a> or explore the navigation above.
    </p>
  </section>
</BaseLayout>
